                <TextView
                    android:id="@+id/go_pay_pay_money"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="10dp"
                    android:layout_marginTop="10dp"
                    android:text="普通配送"
                    android:textSize="13sp" />

                <TextView
                    android:id="@+id/tv_general_unit"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignBottom="@id/go_pay_pay_money"
                    android:layout_marginLeft="10dp"
                    android:layout_marginTop="9dp"
                    android:layout_toRightOf="@id/go_pay_pay_money"
                    android:text="@string/unit"
                    android:textColor="@color/theme"
                    android:textSize="14sp" />

                <TextView
                    android:id="@+id/go_pay_general_price"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignBottom="@id/go_pay_pay_money"
                    android:layout_marginTop="9dp"
                    android:layout_toRightOf="@id/tv_general_unit"
                    android:textColor="@color/theme"
                    android:textSize="14sp"
                    android:textStyle="bold" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignLeft="@id/go_pay_pay_money"
                    android:layout_below="@id/go_pay_pay_money"
                    android:text="系统将根据您的收货地址自动匹配快递公司进行配送，顺丰除外"
                    android:textSize="8sp" />

                <CheckBox
                    android:id="@+id/go_pay_check"
                    style="@style/CustomCheckboxTheme"
                    android:layout_width="@dimen/base_info_match_add"
                    android:layout_height="@dimen/margin_top"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:layout_marginRight="@dimen/width_fu_add"
