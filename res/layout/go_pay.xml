                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="10dp"
                    android:contentDescription="@string/yidejia"
                    android:src="@drawable/alipay" />

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="15dp"
                    android:layout_toRightOf="@id/go_pay_zhifubao_wangyezhifu"
                    android:orientation="vertical" >

                    <TextView
                        android:id="@+id/go_pay_zhifubaowangye"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="支付宝网页支付"
                        android:textSize="13sp" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="可银行卡支付"
                        android:textSize="10sp" />
                </LinearLayout>

                <CheckBox
                    android:id="@+id/zhufubaowangye_checkbox"
                    style="@style/CustomCheckboxTheme"
                    android:layout_width="@dimen/base_info_match_add"
                    android:layout_height="@dimen/margin_top"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:layout_marginRight="@dimen/width_fu_add"
                    android:clickable="true" />
            </RelativeLayout>

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@color/white" >

                <ImageView
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/width_add"
                    android:layout_marginLeft="@dimen/all_buy_produce_right"
                    android:background="@drawable/filter_line"
                    android:contentDescription="@string/yidejia" />
            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/go_pay_zhifubao_caifutong_relative"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@color/white"
                android:padding="@dimen/actiobar_margin_top" >

                <ImageView
                    android:id="@+id/go_pay_zhifubao_caifutong"
