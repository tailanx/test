                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@color/white" >

                <ImageView
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_marginLeft="@dimen/all_buy_produce_right"
                    android:background="@drawable/filter_line"
                    android:contentDescription="@string/yidejia" />
            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/go_pay_zhifubao_wangyezhifu_relative"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@color/white"
                android:padding="@dimen/actiobar_margin_top" >

                <ImageView
                    android:id="@+id/go_pay_zhifubao_wangyezhifu"
