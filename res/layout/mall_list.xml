<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" >

    <RelativeLayout
        android:id="@+id/re_main2_main2_linearlayout4_all_order"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@drawable/layout_selector2" >

        <TextView
            android:id="@+id/tv_main2_main2_linearlayout4_textview1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginLeft="@dimen/margin_left"
            android:text="@string/all_order"
            android:textColor="@color/text_color_80"
            android:textSize="@dimen/textsize_big" />

        <ImageView
            android:id="@+id/tv_main2_main2_linearlayout4_imageview2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:contentDescription="@string/yidejia"
            android:padding="@dimen/mall_list_padding"
            android:src="@drawable/arrow" />
    </RelativeLayout>
  <RelativeLayout
        android:id="@+id/re_main2_main2_linearlayout4_all_show"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/width_add_fun"
        android:background="@drawable/layout_selector2"
       >

        <TextView
            android:id="@+id/tv_main2_main2_linearlayout4_show"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginLeft="@dimen/margin_left"
            android:text="@string/phone_order"
            android:textColor="@color/text_color_80"
            android:textSize="@dimen/textsize_big" />

        <ImageView
            android:id="@+id/tv_main2_main2_linearlayout4_imageview2_show"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:contentDescription="@string/yidejia"
            android:padding="@dimen/mall_list_padding"
            android:src="@drawable/arrow" />
    </RelativeLayout>
    
    <RelativeLayout
        android:id="@+id/re_main2_main2_linearlayout4_integer"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/margin_left"
        android:background="@drawable/layout_selector2" >

        <TextView
            android:id="@+id/tv_main2_main2_linearlayout4_textview1_integer"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginLeft="@dimen/margin_left"
            android:text="@string/main_voucher_text"
            android:textColor="@color/text_color_80"
            android:textSize="@dimen/textsize_big" />

        <ImageView
            android:id="@+id/tv_main2_main2_linearlayout4_imageview2_integer"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:contentDescription="@string/yidejia"
            android:padding="@dimen/mall_list_padding"
            android:src="@drawable/arrow" />
    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/re_my_aidou"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="-1dp"
        android:layout_marginTop="-1dp"
        android:background="@drawable/layout_selector2" >

        <TextView
            android:id="@+id/tv_my_aidou_text"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginLeft="@dimen/margin_left"
            android:text="@string/my_aidou"
            android:textColor="@color/text_color_80"
            android:textSize="@dimen/textsize_big" />

        <ImageView
            android:id="@+id/tv_my_aidou_arrow"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:contentDescription="@string/yidejia"
            android:padding="@dimen/mall_list_padding"
            android:src="@drawable/arrow" />
    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/re_main2_main2_linearlayout4_collect"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="-1dp"
        android:layout_marginTop="-1dp"
        android:background="@drawable/layout_selector2" >

        <TextView
            android:id="@+id/tv_main2_main2_linearlayout4_textview1_collect"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginLeft="@dimen/margin_left"
