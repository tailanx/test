            android:layout_centerVertical="true"
            android:contentDescription="@string/yidejia"
            android:padding="@dimen/mall_list_padding"
            android:src="@drawable/arrow" />
    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/re_main2_main2_linearlayout4_address"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@drawable/layout_selector2" >

        <TextView
            android:id="@+id/tv_main2_main2_linearlayout4_textview1_address"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginLeft="@dimen/margin_left"
            android:text="@string/manage_address"
            android:textColor="@color/text_color_80"
            android:textSize="@dimen/textsize_big" />

        <ImageView
            android:id="@+id/tv_main2_main2_linearlayout4_imageview2_address"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:contentDescription="@string/yidejia"
            android:padding="@dimen/mall_list_padding"
            android:src="@drawable/arrow" />
    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/re_main2_main2_linearlayout4_all_message"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/margin_left"
        android:background="@drawable/layout_selector2" >

        <TextView
            android:id="@+id/tv_main2_main2_linearlayout4_message"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginLeft="@dimen/margin_left"
            android:text="@string/person_message"
            android:textColor="@color/text_color_80"
            android:textSize="@dimen/textsize_big" />

        <ImageView
            android:id="@+id/tv_main2_main2_linearlayout4_imageview2_message"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:contentDescription="@string/yidejia"
            android:padding="@dimen/mall_list_padding"
            android:src="@drawable/arrow" />
    </RelativeLayout>

  
    <RelativeLayout
        android:id="@+id/re_main2_main2_linearlayout4_all_return"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="-1dp"
        android:layout_marginTop="-1dp"
        android:background="@drawable/layout_selector2" >

        <TextView
            android:id="@+id/tv_main2_main2_linearlayout4_return"
